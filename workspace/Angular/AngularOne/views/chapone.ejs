<!DOCTYPE html>
<html >

  <head>
    <script data-require="angular.js@*" data-semver="1.3.0-beta.5" 
            src="//code.angularjs.org/1.3.0-beta.5/angular.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="/stylesheets/style.css"/>
    
  </head>

  <body>
      <div>
        <div ng-app="myApp">
          <h1>Welcome</h1> 
          
          <div ng-controller = "MainController">
      
          <h3>{{ message }}</h3>  
          <br>
          <br>
        <form name="searchUser">
          <h5>Tell Us something about yourself</h5>
          <label><b>Your Username:</b></label>
          <input id="user" placeholder="Search User" ng-model="username" ng-blur="leave()"></input><br><br>
          <label><b>Gender: </b></label>{{gender}} <br> <br>
          <img ng-src={{image}}>
          
          
          {{name}}<br>
          
          <button id="submit">Submit</button>
        </form>
  
        </div>
      </div>
    </div>
  </body>
<script src="/javascripts/Controllers/indexController.js"></script>
</html>
